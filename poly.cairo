func main() {
    // [0 -> x] ap: 0
    [ap] = 100, ap++; 
    // [0 -> x, 1 -> x^2] ap: 1 
    [ap] = [ap-1] * [ap-1], ap++; 
    // [0 -> x, 1 -> x^2, 2 -> x^3] ap: 2
    [ap] = [ap-2] * [ap-1], ap++; 
    // [0 -> x, 1 -> x^2, 2 -> x^3, 3 -> 23 * x^2] ap: 3
    [ap] = [ap-2] * 23, ap++;  
    // [0 -> x, 1 -> x^2, 2 -> x^3, 3 -> 23 * x^2, 4 -> 45 * x] ap: 4
    [ap] = [ap-4] * 45, ap++;  
    // [0 -> x, 1 -> x^2, 2 -> x^3, 3 -> 23 * x^2, 4 -> 45 * x, 5 -> x^3 + (23 * x^2)] ap: 5
    [ap] = [ap-3] + [ap-2], ap++;
    // [0 -> x, 1 -> x^2, 2 -> x^3, 3 -> 23 * x^2, 4 -> 45 * x, 5 -> x^3 + (23 * x^2), 6 -> x^3 + (23 * x^2) + (45 * x)] ap: 6  
    [ap] = [ap-1] + [ap-2], ap++; 
    // [0 -> x, 1 -> x^2, 2 -> x^3, 3 -> 23 * x^2, 4 -> 45 * x, 5 -> x^3 + (23 * x^2), 6 -> x^3 + (23 * x^2) + (45 * x), 7 ->x^3 + (23 * x^2) + (45 * x) + 67] ap: 7
    [ap] = [ap-1] + 67, ap++;  
    ret;
}